  "type": "object",
  "title": "FocusTrap Props",
  "description": "Props for the FocusTrap component",
  "properties": {
    "active": {
      "description": "",
      "type": "boolean",
      "default": true
    },
    "focusTrapOptions": {
      "description": " (Type: FocusTrapOptions)",
